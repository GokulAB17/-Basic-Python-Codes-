      "source": [
        "def p(n):\n",
        "  for i in range(n):\n",
        "    s=\"-\".join(chr(ord(\"a\")+n-j-1) for j in range(i+1))\n",
        "    print((s+s[::-1][1:]).center(n*4-3,\"-\"))\n",
        "  \n",
        "  for i in range(n-1):\n",
        "    s=\"-\".join(chr(ord(\"a\")+n-j-1) for j in range(n-i-1))\n",
        "    print((s+s[::-1][1:]).center(n*4-3,\"-\"))"
      ],
      "execution_count": 1,
      "outputs": []
    },
